                "CATG_DESC": {
                  "type": "string",
                  "default": "AC Filter",
                  "examples": [
                    "AC Filter"
                  ]
                },
                "CATG_CODE": {
                  "type": "string",
                  "default": "ACF",
                  "examples": [
                    "ACF"
                  ]
                },
                "SUBCATG_DETAILS_LIST": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
